{
  "name": "@logicalhq/commitlint-config",
  "version": "1.2.0",
  "author": "Logical <foss@logical.work>",
  "license": "MIT",
  "description": "The shareable commitlint config we use at Logical.",
  "files": [
    "data/**",
    "index.js",
    "LICENSE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logicalhq/commitlint-config.git"
  },
  "bugs": {
    "url": "https://github.com/logicalhq/commitlint-config/issues"
  },
  "homepage": "https://github.com/logicalhq/commitlint-config#readme",
  "keywords": [
    "commintlint-config"
  ],
  "scripts": {
    "fmt": "prettier --write \"./**/*.{js,json,md}\"",
    "fmt:check": "prettier --list-different \"./**/*.{js,json,md}\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:spelling": "cspell --config=.cspell.json \"**/*.{md,js}\"",
    "precommit": "yarn lint-staged",
    "prepush": "yarn fmt:check",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/lint": "^13.1.0",
    "@logicalhq/eslint-config-base": "^1.0.4",
    "cspell": "^5.8.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "**/lodash": "^4.17.19",
    "**/dot-prop": "^5.2.0",
    "**/trim-newlines": "^3.0.1",
    "**/yargs-parser": "^13.1.2"
  }
}
